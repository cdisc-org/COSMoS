            "sdtmVariable": {
               "description": "SDTM target variable for data collection item variable",
               "type": "string"
            }
         },
         "required": [
            "sdtmVariable",
            "sdtmTargetMapping"
         ],
         "title": "SDTMTarget",
         "type": "object"
      }
