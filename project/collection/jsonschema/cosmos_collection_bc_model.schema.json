         "type": "string"
      },
      "CodeList": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "conceptId": {
               "description": "C-code for a codelist in NCIt",
               "pattern": "^(C[0123456789]+)$",
               "type": "string"
            },
            "href": {
               "description": "Link to NCIt for the codelist",
               "type": "string"
            },
            "submissionValue": {
               "description": "CDISC submission value for the codelist",
