         "type": "object"
      },
      "ListTypeEnum": {
         "description": "",
         "enum": [
            "Radio",
            "Dropdown",
            "DropdownMultiSelect",
            "Checkbox",
            "Text",
            "Date",
            "Time",
            "DateTime"
         ],
         "title": "ListTypeEnum",
         "type": "string"
      },
      "ListValue": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "displayValue": {
               "description": "User-friendly display value for the data collection item",
               "type": "string"
            },
            "value": {
               "description": "CDISC submission value for the data collection item",
               "type": "string"
            }
         },
         "required": [
            "displayValue"
         ],
         "title": "ListValue",
         "type": "object"
      },
      "PackageTypeEnum": {
         "description": "",
         "enum": [
            "collection"
         ],
         "title": "PackageTypeEnum",
         "type": "string"
      },
      "PrepopulatedValue": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "conceptId": {
               "description": "C-code for assigned term in NCIt or left blank when CDISC terminology does not apply",
               "pattern": "^(C[0123456789]+)$",
               "type": "string"
            },
            "value": {
               "description": "Submission value for assigned term in NCIt if it exists, or an assigned value which will be the default value",
               "type": "string"
            }
         },
         "required": [
            "value"
         ],
         "title": "PrepopulatedValue",
         "type": "object"
      },
      "SDTMTarget": {
         "additionalProperties": false,
         "description": "",
         "properties": {
            "sdtmAnnotation": {
