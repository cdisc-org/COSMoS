            sh:maxCount 1 ;
            sh:order 13 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:mandatoryVariable> ],
        [ sh:description "Indicator that variable must be populated within the SDTM group" ;
            sh:maxCount 1 ;
            sh:order 14 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:mandatoryValue> ],
        [ sh:description "SDTM variable role" ;
            sh:maxCount 1 ;
