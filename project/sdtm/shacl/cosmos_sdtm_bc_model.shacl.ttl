            sh:maxCount 1 ;
            sh:order 12 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:significantDigits> ],
        [ sh:description "Target variable for VLM" ;
            sh:maxCount 1 ;
            sh:order 18 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:vlmTarget> ],
        [ sh:description "Comparison operator for SDTM group variables included in VLM" ;
            sh:in ( "EQ" "IN" ) ;
            sh:maxCount 1 ;
            sh:order 17 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:comparator> ],
        [ sh:description "Variable length" ;
            sh:maxCount 1 ;
            sh:minInclusive 1 ;
            sh:order 10 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:length> ],
