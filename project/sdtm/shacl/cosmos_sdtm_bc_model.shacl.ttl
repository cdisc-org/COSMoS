            sh:minCount 1 ;
            sh:order 1 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:value> ] ;
    sh:targetClass <https://www.cdisc.org/cosmos/1-0/:AssignedTerm> .

<https://www.cdisc.org/cosmos/1-0/:CodeList> a sh:NodeShape ;
