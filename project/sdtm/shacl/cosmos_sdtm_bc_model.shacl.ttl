            sh:nodeKind sh:IRI ;
            sh:order 6 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:assignedTerm> ],
        [ sh:description "Variable display format" ;
            sh:maxCount 1 ;
            sh:order 11 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:format> ],
        [ sh:description "Variable origin source (define-XML v21)" ;
