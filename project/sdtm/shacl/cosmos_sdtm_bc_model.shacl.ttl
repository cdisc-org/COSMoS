<https://www.cdisc.org/cosmos/1-0/:RelationShip> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Variable relationship descriptive linking phrase" ;
            sh:in ( "decodes the value in" "is decoded by the value in" "groups values in" "is the subject position during performance of the test in" "is the result of the test in" "is the unit for the value in" "is the specimen tested in" "specifies the anatomical location of the performance of the test in" "further specifies the anatomical location in" "is the method for the test in" "is the subject's fasting status during the performance of the test in" "identifies an observation described by" "is the dictionary code for the test in" "is the role of the assessor who performed the test in" "is the epoch of the performance of the test in" "identifies overall response supported by tumor assessments identified by" "identifies the image from the procedure in" "identifies the tumor found by the test in" "groups tumor assessments used in overall response identified by" "is the identifier for the source data used in the performance of the test in" "associates the tumor identified in" "indicates the previous irradiation status of the tumor identified by" "indicates the progression status of the previous irradiated tumor identified by" "further describes the test in" "is a dictionary-derived term for the value in" "is original text for" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:linkingPhrase> ],
        [ sh:description "Subject in a variable relationship" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:subject> ],
        [ sh:description "Short variable relatio ship linking phrase for programming purposes" ;
            sh:in ( "DECODES" "IS_DECODED_BY" "GROUPS" "IS_SUBJECT_STATE_FOR" "IS_RESULT_OF" "IS_UNIT_FOR" "IDENTIFIES_OBSERVATION" "IS_SPECIMEN_TESTED_IN" "SPECIFIES" "CODES" "PERFORMED" "IS_EPOCH_OF" "GROUPS_BY" "IDENTIFIES_PRODUCT_IN" "IDENTIFIES_TUMOR_IN" "IS_ORIGINAL_TEXT_FOR" "IS_DERIVED_FROM" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:predicateTerm> ],
        [ sh:description "Object in a variable relationship" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:object> ] ;
    sh:targetClass <https://www.cdisc.org/cosmos/1-0/:RelationShip> .

<https://www.cdisc.org/cosmos/1-0/:SDTMVariable> a sh:NodeShape ;
    sh:closed true ;
