            sh:order 3 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:codelistTerm> ],
        [ sh:description "Subset codelist label" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:subsetLabel> ],
        [ sh:description "Subset codelist parent codelist" ;
            sh:maxCount 1 ;
