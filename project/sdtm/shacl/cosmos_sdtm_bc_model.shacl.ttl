@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://www.cdisc.org/cosmos/1-0/:SDTMGroup> a sh:NodeShape ;
    sh:closed true ;
    sh:ignoredProperties ( rdf:type ) ;
    sh:property [ sh:description "Identifier for SDTM Value Level Metadata group" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 4 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:datasetSpecializationId> ],
        [ sh:description "The earliest SDTMIG version applicable to the BC dataset specialization" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 6 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:sdtmigStartVersion> ],
        [ sh:class <https://www.cdisc.org/cosmos/1-0/:SDTMVariable> ;
            sh:description "Variable included in the SDTM dataset specialization" ;
            sh:nodeKind sh:IRI ;
            sh:order 9 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:variables> ],
        [ sh:description "Biomedical Concept package release date indicating when the BC package was published to production" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:packageDate> ],
        [ sh:description "Domain for the SDTM specialization group" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 2 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:domain> ],
        [ sh:description "SDTM group short name which provides a user friendly and intuitive name for the vlm_group_id" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 3 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:shortName> ],
        [ sh:description "The last SDTMIG version that is applicable to the BC dataset specialization" ;
            sh:maxCount 1 ;
            sh:order 7 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:sdtmigEndVersion> ],
        [ sh:description "Package type (sdtm for SDTM Dataset Specializations)" ;
            sh:in ( "sdtm" ) ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 1 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:packageType> ],
        [ sh:description "Biomedical Concept identifier" ;
            sh:maxCount 1 ;
            sh:order 8 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:biomedicalConceptId> ],
        [ sh:description "SDTM VLM Source which categorizes VLM groups by topic variable" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 5 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:source> ] ;
    sh:targetClass <https://www.cdisc.org/cosmos/1-0/:SDTMGroup> .

<https://www.cdisc.org/cosmos/1-0/:SubsetCodeList> a sh:NodeShape ;
