        [ sh:description "Variable data type" ;
            sh:in ( "float" "integer" "text" ) ;
            sh:maxCount 1 ;
            sh:order 9 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:dataType> ],
        [ sh:description "Variable included in the SDTM dataset specialization" ;
            sh:maxCount 1 ;
            sh:minCount 1 ;
            sh:order 0 ;
            sh:path <https://www.cdisc.org/cosmos/1-0/:name> ],
